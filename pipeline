pipeline { 
    agent any
    stages {
        stage('Clone Git') {
            steps {
                git 'https://github.com/Shreyansh-Rai/SELab-6-Jenkins.git'
            }
        }
        stage('Build Code') {
            steps {
                sh "chmod u+x code.py code2.py"
                sh "python3 code.py"
                sh "python3 code2.py"
            }
        }
     stage('Test Code') {
            steps {
                sh "chmod u+x tests.py"
                sh "./tests.py"
            }
        }
    } 
}